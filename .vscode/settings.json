{
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "flake8.args": [
        "--max-line-length", "88",
        "--extend-ignore", "E203"
       ]
}
